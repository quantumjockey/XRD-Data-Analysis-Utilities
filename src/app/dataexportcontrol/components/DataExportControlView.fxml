<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>

<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.dataexportcontrol.components.DataExportControlController">
    <children>
        <HBox alignment="CENTER" spacing="2.0">
            <ChoiceBox fx:id="exportOptions" />
            <Button mnemonicParsing="false" onAction="#exportImage" text="Export" />
        </HBox>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</fx:root>
