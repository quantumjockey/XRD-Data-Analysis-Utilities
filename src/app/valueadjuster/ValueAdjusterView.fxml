<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>

<fx:root spacing="5.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.valueadjuster.ValueAdjusterController">
    <children>
        <Button mnemonicParsing="false" onAction="#Increment" text="+" />
        <TextField fx:id="value" prefWidth="100.0" />
        <Button mnemonicParsing="false" onAction="#Decrement" text="-" />
    </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</fx:root>
